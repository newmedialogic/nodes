<h1>Nodes Plugin app/views/nodes/index.html.erb</h1>
<p>
<%= Nodes.node_classes.map(&:to_s) %>
</p>
<table>
  <thead>
    <th>ID</th>
    <th>Title</th>
    <th>Date</th>
  </thead>
  <% @nodes.each do |node| %>
  <tbody>
    <tr>
      <td><%= node.id %></td>
      <td><%= node.title %></td>
      <td><%= node.updated_at %></td>
    </tr>
  </tbody>
<% end %>
</table>
